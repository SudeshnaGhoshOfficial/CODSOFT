        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "\n",
        "# Create a TfidfVectorizer object with specified parameters\n",
        "vectorizer = TfidfVectorizer(ngram_range=(1, 2), max_features=5000, min_df=2)\n",
